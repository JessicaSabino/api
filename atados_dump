CREATE TABLE atados_core_address (
    id integer NOT NULL,
    zipcode character varying(10),
    addressline character varying(200),
    addressnumber character varying(10),
    addressline2 character varying(100),
    neighborhood character varying(50),
    city_id integer,
    latitude double precision,
    longitude double precision
);


ALTER TABLE public.atados_core_address OWNER TO marjoripomarole;

--
-- Name: atados_core_address_id_seq; Type: SEQUENCE; Schema: public; Owner: marjoripomarole
